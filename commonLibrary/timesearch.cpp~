#include "timesearch.cpp"
#include <cmath>
timesearch::timesearch(){
	lastResors=0;
	lastResors=0;
	lastDelta=0;
}

bool timesearch::update2ndResor_GivenFidelity(double curResors, double curFidR, double targetFidR}{
   double xx=0.2;
   double xxx=0.1
   double nextResors=0;
   double delta=0;
   if (abs(targetFidR-curFidR)<fidError) return true;
   if(lastDelta!=0){
	double dFidR=curFidR-lastFidR;
	double dResource=curResors-lastResors;
	double unit=dResource/dFidR;
	delta=(targetFidR-curFidR)/dFidR*unit;
	nextResors=curResors+delta;
   }
   else {
	nextResors=((curFidR<targetFidR)?curResors+xx:curResors-xx)
   }  
   return false;
}


	
